{
    "name": "mattvick/bolt-diy-forms",
    "description": "Example bolt extension demonstrating how to bind a form to a route and submit it's data to a contenttype.",
    "type": "bolt-extension",
    "require": {
        "bolt/bolt": ">=2.0.0,<3.0.0"
    },
    "license" : "GPL-3.0+",
    "authors": [
        {
            "name": "Matthew Vickery", 
            "email": "vickery.matthew@gmail.com"
        }
    ],
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "autoload": {
        "files": [
            "init.php"
        ],
        "psr-4": {
            "Bolt\\Extension\\Mattvick\\DiyForms\\": "src"
        }
    },
    
    "extra": {
        "bolt-assets": "assets"
    }
}
